pipeline {
    agent any
    options {
        skipStagesAfterUnstable()
    }
    stages {
        stage('Build') {
            steps {
				bat 'cd SoftwareIntegration'
                bat 'mvn -B -DskipTests clean package'
            }
        }
        stage('Test') {
            steps {
				bat 'cd SoftwareIntegration'
                bat 'mvn test'
            }
            post {
                always {
                    junit 'target/surefire-reports/*.xml'
                }
            }
        }
        stage('Deliver') { 
            steps {
				bat 'cd SoftwareIntegration'
                bat './jenkins/scripts/deliver.sh' 
            }
        }
    }
}